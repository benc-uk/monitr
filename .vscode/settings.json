{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  // Pretend we're not using NPM
  "zfiles.exclude": {
    "**/package*.json": true,
    "**/node_modules": true,
  },

  // Override icons for specific file types
  "material-icon-theme.files.associations": {
    "*.tsp": "xml"
  }
}